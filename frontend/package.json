{
  "name": "@jellyfin-vue/frontend",
  "version": "0.3.1",
  "private": true,
  "type": "module",
  "browserslist": [
    "defaults and supports es6-module"
  ],
  "scripts": {
    "analyze": "dpdm --no-warning --no-tree src/**/*.{ts,vue} && vite build --mode analyze",
    "build": "vite build",
    "check": "npm run lint && npm run typecheck",
    "start": "vite",
    "serve": "vite preview",
    "lint": "eslint . --max-warnings=0 --cache --cache-location ../node_modules/.cache/eslint",
    "lint-fix": "eslint . --fix --max-warnings=0 --cache --cache-location ../node_modules/.cache/eslint",
    "eslint-inspect": "eslint-config-inspector",
    "prod": "npm run build && npm run serve",
    "typecheck": "vue-tsc --incremental",
    "clean": "git clean -fxd"
  },
  "dependencies": {
    "@fontsource-variable/figtree": "5.0.21",
    "@jellyfin/sdk": "0.10.0",
    "@skirtle/vue-vnode-utils": "0.1.4",
    "@vueuse/components": "10.11.0",
    "@vueuse/core": "10.11.0",
    "audiomotion-analyzer": "4.5.0",
    "axios": "1.7.2",
    "blurhash": "2.0.5",
    "comlink": "4.4.1",
    "date-fns": "3.6.0",
    "defu": "6.1.4",
    "destr": "2.0.3",
    "dompurify": "3.1.5",
    "fast-equals": "5.0.1",
    "hls.js": "1.5.11",
    "jassub": "1.7.15",
    "lodash-es": "4.17.21",
    "marked": "13.0.0",
    "radix-vue": "1.8.3",
    "sortablejs": "1.15.2",
    "swiper": "11.1.4",
    "uuid": "10.0.0",
    "vue": "3.4.29",
    "vue-i18n": "9.13.1",
    "vue-router": "4.3.3",
    "vuetify": "3.5.18"
  },
  "devDependencies": {
    "@eslint/config-inspector": "0.4.10",
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.4.0",
    "@iconify/json": "2.2.217",
    "@intlify/eslint-plugin-vue-i18n": "3.0.0",
    "@intlify/unplugin-vue-i18n": "4.0.0",
    "@rollup/plugin-virtual": "3.0.2",
    "@stylistic/eslint-plugin": "2.1.0",
    "@types/dompurify": "3.0.5",
    "@types/lodash-es": "4.17.12",
    "@types/node": "20.14.2",
    "@types/sortablejs": "1.15.8",
    "@types/uuid": "9.0.8",
    "@vitejs/plugin-vue": "5.0.5",
    "browserslist": "4.23.1",
    "dpdm": "3.14.0",
    "eslint": "9.4.0",
    "eslint-config-flat-gitignore": "0.1.5",
    "eslint-plugin-css": "0.9.2",
    "eslint-plugin-file-progress": "1.4.0",
    "eslint-plugin-import-x": "0.5.1",
    "eslint-plugin-jsdoc": "48.2.9",
    "eslint-plugin-jsonc": "2.16.0",
    "eslint-plugin-optimize-regex": "1.2.1",
    "eslint-plugin-promise": "6.2.0",
    "eslint-plugin-sonarjs": "1.0.3",
    "eslint-plugin-unicorn": "53.0.0",
    "eslint-plugin-vue": "9.26.0",
    "eslint-plugin-vue-scoped-css": "2.8.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.14.0",
    "globals": "15.4.0",
    "lightningcss": "1.25.1",
    "rollup-plugin-visualizer": "5.12.0",
    "typescript": "5.4.5",
    "typescript-eslint": "7.12.0",
    "unocss": "0.60.4",
    "unplugin-icons": "0.19.0",
    "unplugin-vue-components": "0.27.0",
    "unplugin-vue-macros": "2.9.4",
    "unplugin-vue-router": "0.9.1",
    "vite": "5.2.13",
    "vue-eslint-parser": "9.4.3",
    "vue-tsc": "2.0.21"
  }
}
