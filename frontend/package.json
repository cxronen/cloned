    "prod": "npm run build && npm run serve",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "vue-i18n-extract": "vue-i18n-extract",
    "vue-i18n-extract-remove": "vue-i18n-extract --remove"
  },
