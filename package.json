    "destr": "^1.1.0",
    "qs": "^6.10.1",
    "swiper": "^6.7.1",
    "vue": "^3.0.5",
    "vue-i18n": "^9.1.6",
    "vue-router": "^4.0.10",
    "vuetify": "^3.0.0-alpha.7",
    "vuex": "^4.0.2",
    "vuex-composition-helpers": "^2.0.0"
